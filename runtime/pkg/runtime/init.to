
use os
use std
use string

func args_init(argc<u64> argv<u64*>){
	c = int(argc)
	c -= 1

	arr = []
	while argc > 0 {
		if *argv == null {
			break
		}
		str<Value> = new Value
		str.type = String
		str.data = string.stringnew(*argv)
		arr_pushone(arr,str)
		// 8字节指针
		argv += PointerSize
		argc -= 1
	}
	os.argc = c 
	os.argv = arr

}