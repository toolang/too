package main

import fmt
import os

func main(){
    fmt.sprintf("bool test\n")

    // 测试 bool 赋值  和 istrue判断
    isbool = true
    if  isbool {
        fmt.sprintf("bool test ok\n")
    }else{
        fmt.sprintf("bool test failed\n")
        os.exit(1)
    }

    // 测试 bool 赋值  和 istrue判断
    isbool = false
    if  isbool {
        fmt.sprintf("bool test failed\n")
        os.exit(1)
    }else{
        fmt.sprintf("bool test ok\n")
    }


}
